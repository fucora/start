<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>io.basc.framework</groupId>
		<artifactId>framework</artifactId>
		<version>1.8.0</version>
	</parent>
	<groupId>io.basc.integration</groupId>
	<artifactId>open-platform</artifactId>
	<packaging>pom</packaging>
	<description>对第三方开放平台的整合</description>
	
	<properties>
		<basc.version>1.8.0</basc.version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>io.basc.framework</groupId>
			<artifactId>core</artifactId>
		</dependency>

		<dependency>
			<groupId>io.basc.framework</groupId>
			<artifactId>validation</artifactId>
		</dependency>

		<dependency>
			<groupId>io.swagger.core.v3</groupId>
			<artifactId>swagger-annotations</artifactId>
		</dependency>
	</dependencies>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>io.basc.integration</groupId>
				<artifactId>trade</artifactId>
				<version>${basc.version}</version>
			</dependency>
			<dependency>
				<groupId>io.basc.integration</groupId>
				<artifactId>alibaba</artifactId>
				<version>${basc.version}</version>
			</dependency>
			<dependency>
				<groupId>io.basc.integration</groupId>
				<artifactId>tencent</artifactId>
				<version>${basc.version}</version>
			</dependency>
			<dependency>
				<groupId>io.basc.integration</groupId>
				<artifactId>kdniao</artifactId>
				<version>${basc.version}</version>
			</dependency>
			<dependency>
				<groupId>io.basc.integration</groupId>
				<artifactId>apple</artifactId>
				<version>${basc.version}</version>
			</dependency>
			<dependency>
				<groupId>io.basc.integration</groupId>
				<artifactId>bytedance</artifactId>
				<version>${basc.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

	<modules>
		<module>trade</module>
		<module>tencent</module>
		<module>alibaba</module>
		<module>kdniao</module>
		<module>apple</module>
		<module>bytedance</module>
	</modules>
</project>